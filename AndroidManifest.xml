<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.h0rn3t.dualbootsplus"
    android:versionCode="4"
    android:versionName="2.0">
    <uses-sdk
        android:minSdkVersion="9"
        android:targetSdkVersion="19"/>

    <uses-permission android:name="android.permission.WRITE_SETTINGS"/>
    <uses-permission android:name="android.permission.ACCESS_SUPERUSER" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WAKE_LOCK"/>
    <uses-permission android:name="android.permission.VIBRATE" />
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/Theme_Dark">
        <activity android:name="com.h0rn3t.dualbootsplus.MainActivity"
            android:screenOrientation="portrait"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.h0rn3t.dualbootsplus.PartActivity"
            android:screenOrientation="portrait"/>
        <activity android:name="com.h0rn3t.dualbootsplus.FileChooser"
            android:screenOrientation="portrait"/>
        <activity android:name="com.h0rn3t.dualbootsplus.AboutActivity"
            android:screenOrientation="portrait"/>
        <activity android:name="com.h0rn3t.dualbootsplus.ToolsActivity"
            android:screenOrientation="portrait"/>
        <activity android:name="com.h0rn3t.dualbootsplus.RestoreActivity"
            android:screenOrientation="portrait"/>
        <activity android:name="com.h0rn3t.dualbootsplus.BackupActivity"
            android:screenOrientation="portrait"/>


    </application>

</manifest>
